{
    "alias":["celineb.me"]
    "version": 2,
    "name": "celineb",
    "builds": [
        { "src": "next.config.js", "use": "@now/next" }
    ],
    "routes":[
        {"src": "/page/(?<slug>.+)", "dest": "/post.js?slug=$slug"},
        {"src": "/post/(?<slug>.+)", "dest": "/post.js?slug=$slug"},
        {"src": "/(.*)", "dest": "/$1"}
    ]
}
