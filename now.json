{
  "alias": ["celineb.me"],
  "version": 2,
  "name": "celineb",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    {"src": "/robots.txt", "dest": "/robots.txt"},
    {"src": "/sitemap.xml", "dest": "/sitemap.xml"},
    {"src": "/post/(?<slug>[^/]+)$", "dest": "/post?slug=$slug"},
    {"src": "/project/(?<slug>[^/]+)$", "dest": "/project?slug=$slug"},
    {"src": "/(?<slug>[^/]+)$", "dest": "/page?slug=$slug"}
  ]

}
